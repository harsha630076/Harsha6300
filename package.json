{
  "name": "quickcal-ai-health-tracker",
  "private": true,
  "type": "module",
  "version": "1.0.0",
  "description": "AI-powered nutrition tracking and health management app with food scanning and mood-based recommendations",
  "author": "QuickCal AI Team",
  "homepage": "https://quickcalai.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/quickcalai/app.git"
  },
  "keywords": [
    "health",
    "nutrition",
    "ai",
    "food-tracking",
    "mobile-app",
    "pwa",
    "fitness",
    "wellness"
  ],
  "pkg": {
    "assets": [
      "dist/spa/*",
      "data/*"
    ],
    "scripts": [
      "dist/server/**/*.js"
    ]
  },
  "scripts": {
    "dev:server": "nodemon --watch server/src --exec tsx server/src/server.ts",
    "dev:client": "vite",
    "dev": "concurrently \"npm:dev:server\" \"npm:dev:client\"",
    "prisma:generate": "prisma generate --schema=server/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=server/prisma/schema.prisma",
    "prisma:seed": "ts-node server/prisma/seed.ts",
    "build": "vite build && npm run build:server",
    "build:client": "vite build",
    "build:server": "tsc -p server/tsconfig.json",
    "start": "node server/dist/server.js",
    "test": "vitest --run",
    "format.fix": "prettier --write .",
    "typecheck": "tsc",
    "mobile:init": "npx cap init 'QuickCal AI' 'com.quickcalai.app'",
    "mobile:add-android": "npx cap add android",
    "mobile:add-ios": "npx cap add ios",
    "mobile:build": "npm run build && npx cap copy",
    "mobile:android": "npx cap open android",
    "mobile:ios": "npx cap open ios",
    "mobile:sync": "npx cap sync",
    "pwa:build": "npm run build && workbox generateSW workbox-config.js",
    "serve": "npm run build && npm start",
    "preview": "vite preview",
    "db:init": "node -e \"require('./server/database/connection.ts').initializeDatabase()\"",
    "db:seed": "node scripts/seed-database.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "icons:generate": "node scripts/generate-icons.js",
    "postinstall": "prisma generate --schema=server/prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "bcryptjs": "^2.4.3",
    "clsx": "^2.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^7.4.1",
